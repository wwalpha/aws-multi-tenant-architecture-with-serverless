{
  "name": "tenant-registry",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "build": "tsc -p tsconfig.build.json",
    "build:docker": "cd .. && docker build --build-arg TARGET_FOLDER=tenant-registry -t saas/tenant-registry -f ./tenant-registry/Dockerfile .",
    "debug": "nodemon",
    "test": "jest"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@tsconfig/node14": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "aws-sdk": "^2.853.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dynamodb-helper": "https://github.com/wwalpha/dynamodb-helper.git",
    "express": "^4.17.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node -r dotenv/config src/index.ts"
  }
}
